<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
  



<servlet>
	<servlet-name>AccountsServlet</servlet-name>
	<servlet-class>com.mancini.prova0.server.AccountServicesImpl</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>AccountsServlet</servlet-name>
	<url-pattern>/prova0/accounts</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>AuthServlet</servlet-name>
	<servlet-class>com.mancini.prova0.server.AuthServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>AuthServlet</servlet-name>
	<url-pattern>/auth/*</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>AuthServletCallback</servlet-name>
	<servlet-class>com.mancini.prova0.server.AuthServletCallback</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>AuthServletCallback</servlet-name>
	<url-pattern>/oauth2callback/*</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>HostPageWriter</servlet-name>
	<servlet-class>com.mancini.prova0.server.HostPage</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>HostPageWriter</servlet-name>
	<url-pattern></url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>Logout</servlet-name>
	<servlet-class>com.mancini.prova0.server.Logout</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Logout</servlet-name>
	<url-pattern>/logout</url-pattern>
</servlet-mapping>



</web-app>
